@page
@model DetailsModel

@{
    ViewData["Title"] = "User's details";
    
}

<h4>@ViewData["Title"]</h4>
<partial name="_StatusMessage" model="Model.StatusMessage" />
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            FirstName
        </dt>
        <dd class="col-sm-10">
            @Model.User.FirstName
        </dd>
        <dt class="col-sm-2">
            LastName
        </dt>
        <dd class="col-sm-10">
           @Model.User.LastName
        </dd>
        
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.User.Email
        </dd>
        <dt class="col-sm-2">
            EmailConfirmed
        </dt>
        <dd class="col-sm-10">
            @Model.User.EmailConfirmed
        </dd>
        <dt class="col-sm-2">
            PhoneNumber
        </dt>
        <dd class="col-sm-10">
           @Model.User.PhoneNumber
        </dd>
       
    </dl>
</div>
<div>
    <a asp-page="Edit" asp-route-id="@Model.User.Id">Edit</a> |
    <a asp-page="UsersList">Back to List</a>
</div>
